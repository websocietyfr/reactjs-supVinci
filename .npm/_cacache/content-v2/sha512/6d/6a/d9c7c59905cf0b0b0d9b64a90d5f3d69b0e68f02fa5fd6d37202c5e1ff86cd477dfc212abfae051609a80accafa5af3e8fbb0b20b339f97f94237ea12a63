 pause, use `this.emit('data', data)`,\nand this.emit('end')\n\n``` js\nvar through = require('through')\n\nthrough(function write(data) {\n    this.emit('data', data)\n    //this.pause() \n  },\n  function end () { //optional\n    this.emit('end')\n  })\n```\n\n## Extended Options\n\nYou will probably not need these 99% of the time.\n\n### autoDestroy=false\n\nBy default, `through` emits close when the writable\nand readable side of the stream has ended.\nIf that is not desired, set `autoDestroy=false`.\n\n``` js\nvar through = require('through')\n\n//like this\nvar ts = through(write, end, {autoDestroy: false})\n//or like this\nvar ts = through(write, end)\nts.autoDestroy = false\n```\n\n## License\n\nMIT / Apache2\n","readmeFilename":"readme.markdown","users":{"306766053":true,"luk":true,"pid":true,"amio":true,"j.su":true,"miju":true,"n370":true,"noim":true,"jpepe":true,"jruif":true,"jxson":true,"romac":true,"456wyc":true,"akarem":true,"bertuz":true,"bojand":true,"bourne":true,"bspink":true,"dubban":true,"hughsk":true,"joakin":true,"joshmu":true,"kastor":true,"lixt07":true,"majgis":true,"monjer":true,"mrzmmr":true,"scull7":true,"season":true,"sloanb":true,"tarcio":true,"x4devs":true,"zxbing":true,"akilism":true,"isaacbw":true,"kahboom":true,"kerry95":true,"mikelin":true,"patrick":true,"soliury":true,"1cr18ni9":true,"ahvonenj":true,"alanshaw":true,"alexkval":true,"carlos8f":true,"chrisayn":true,"esundahl":true,"jmsherry":true,"liveinjs":true,"nicknaso":true,"shtylman":true,"tdmalone":true,"aliorouji":true,"bmpvieira":true,"chriscalo":true,"cilindrox":true,"edwardxyt":true,"evanyeung":true,"magicxiao":true,"max_devjs":true,"nice_body":true,"ninozhang":true,"pixeleate":true,"chinaqstar":true,"chirag8642":true,"claudiopro":true,"coderaiser":true,"davidrapin":true,"juicypasta":true,"lewisbrown":true,"nickleefly":true,"pengzhisun":true,"princetoad":true,"rmilbourne":true,"sanketss84":true,"zousandian":true,"andrewtlove":true,"blakeembrey":true,"caffellatte":true,"flump